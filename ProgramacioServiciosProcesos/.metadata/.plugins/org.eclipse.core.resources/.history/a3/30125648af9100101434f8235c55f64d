package EjerciciosT1;

import java.util.Scanner;

public class ejercicio12 {

	public static void main(String[] args) {
		
		int num;
		
		for (int i = 0; i < 10; i++) {
			Scanner teclado = new Scanner(System.in);
			num = teclado.nextInt();
			
			if (num < 0 || num > 10) {
				System.out.println("Numero debe estar entre 0 y 10");
			} else if (num >= 0 && num <= 4) {
				System.out.println("Nota: Supendido.");
			} else if (num >= 5 && num <= 6) {
				System.out.println("Nota: Aprovado");
			} else if (num >= 7 && num <= 8) {
				System.out.println("Nota: Notable");
			} else if (num == 9) {
				System.out.println("Nota: Matricula");
			}
		}

	}

}
