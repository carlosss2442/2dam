package EjerciciosT1;

import java.util.Scanner;

public class ejercicio13 {

	public static void main(String[] args) {
		
		double fahren = 0;
		double resultado  = 0;
		Scanner teclado = new Scanner(System.in);
		double celcius = teclado.nextDouble();
		
		fahren = (celcius * (9/5)) + 32;
		
	}

}
