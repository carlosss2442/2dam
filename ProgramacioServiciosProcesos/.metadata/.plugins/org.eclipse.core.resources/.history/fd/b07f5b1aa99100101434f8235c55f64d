package EjerciciosT1;

import java.util.Scanner;

public class ejercicio9 {

	public static void main(String[] args) {
		
		int num;
		Scanner teclado = new Scanner(System.in);
		System.out.println("Introduce un numero del 1 al 12: ");
		num = teclado.nextInt();
		
		switch (num) {
		
		case 1:
			System.out.println("Mes: Enero");
			break;
		case 2:
			System.out.println("Mes: Enero");
			break;
		case 3:
			System.out.println("Mes: Enero");
			break;
		case 4:
			System.out.println("Mes: Enero");
			break;
		case 5:
			System.out.println("Mes: Enero");
			break;
		case 6:
			System.out.println("Mes: Enero");
			break;
		case 7:
			System.out.println("Mes: Enero");
			break;
		case 8:
			System.out.println("Mes: Enero");
			break;
		case 9:
			System.out.println("Mes: Enero");
			break;
		case 10:
			System.out.println("Mes: Enero");
			break;
		case 11:
			System.out.println("Mes: Enero");
			break;
		case 12:
			System.out.println("Mes: Enero");
			break;
		
		}
		
	}

}
